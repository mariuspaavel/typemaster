CREATE OR REPLACE FUNCTION getswipes(
	a_accountid INTEGER
)
RETURNS TABLE(
	accountid INTEGER,
	name VARCHAR(20),
	birth BIGINT,
	gender VARCHAR(1),
	bio VARCHAR(256)
)
LANGUAGE plpgsql
AS $$
BEGIN
	RETURN QUERY SELECT accounts.accountid, accounts.name, accounts.birth, accounts.gender, accounts.bio FROM accounts WHERE accounts.accountid <> a_accountid ORDER BY random() limit 3;
END;
$$;

