CREATE OR REPLACE FUNCTION mediaownershipcheck(
	aaccountid INTEGER,
	amediaid INTEGER
)
RETURNS void
LANGUAGE plpgsql
AS $$
DECLARE 
realowner INTEGER;
BEGIN
SELECT owner INTO realowner FROM media WHERE mediaid=amediaid;
IF aaccountid <> realowner THEN
	RAISE 'not your fucking photo';
END IF;
END;
$$;
